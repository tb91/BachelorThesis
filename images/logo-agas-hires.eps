%BeginProlog
/swid 7 def
/cwid 131 def
/chig 211 def
/xpos0 0 def
/step 13 def

/h1o cwid 2 div def
/h1i cwid 2 div swid 2 sqrt mul sub def
/h1oS h1o 2 sqrt div def
/mhig chig h1oS sub 2.05 div h1oS  add def

/xpos1 xpos0 cwid add step add def
/xpos2 xpos1 cwid add step 7 add add def
/xpos3 xpos2 cwid add step add def
/xpos4 xpos3 cwid add cwid 0.52 mul add def
/gypos chig 27.5 sub def
/gxpos xpos1 cwid 0.62 mul add def
/axpos xpos0 cwid 0.25 mul add def
/syposu chig 40 sub def
/syposl h1oS 40 add def

/genColImg {
	15 dict begin
	  107 154 232		% light blue
	  /bs exch def /gs exch def /rs exch def 
	  21 61 208		% dark blue
	  /be exch def /Ge exch def /re exch def	% ge is operator, use Ge
	  /rd re rs sub def
	  /gd Ge gs sub def
	  /bd be bs sub def

	  /steps exch def
	  mark
	  0 1 steps {
		/f exch steps div def
		% just put values on stack
		rs rd f mul add round cvi
		gs gd f mul add round cvi
		bs bd f mul add round cvi
	  } for

	  /n steps 1 add 3 mul def
	  /s n string def
	  /i n 1 sub def
	  counttomark {
         s i 3 -1 roll put
		/i i 1 sub def
	  } repeat
	  pop % remove mark
	  s	% put string on stack
	end
} bind def

% Viertelkreis mit Bezier-Kurve, r=1:
% 0 0 moveto 0 A 1 A sub 1 1 1 curveto
/A 8 2 sqrt div 4 sub 3 div def

/halfcircR {	% halbkreis von links nach rechts /\
  8 dict begin
    /rv exch def	% radius vertikal
    /rh exch def	% radius horizontal
    currentpoint /y exch def /x exch def	% cp0 = start
    /xm x rh add def /ym y rv add def		% pos middle
    /xe x rh 2 mul add def /ye y def		% pos end
    x                 y A rv mul add	% cp1
    xm A rh mul sub   ym		% cp2
    xm                ym		% cp3
    curveto
    % currenpoint now == (xm,ym)
    xm A rh mul add   ym
    xe                ye A rv mul add
    xe                ye
    curveto
  end
} bind def

